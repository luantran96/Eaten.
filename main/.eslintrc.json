{
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "rules": {
        "no-console": 0,
        "prettier/prettier": ["error"]
    },
    "env": {
        "browser": true,
        "jest": true
    }
 }